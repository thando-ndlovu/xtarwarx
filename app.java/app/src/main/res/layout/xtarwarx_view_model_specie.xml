<?xml version="1.0" encoding="utf-8"?>
<merge
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.appcompat.widget.AppCompatTextView
        style="@style/XtarWarx.View.Model.Text.Header"
        android:id="@+id/xtarwarx_view_model_specie_name_appcompattextview"
        android:layout_width="@dimen/dp0"
        android:layout_height="@dimen/dp0"
        android:layout_marginVertical="@dimen/dp16"

        app:layout_constraintWidth="match_constraint"
        app:layout_constraintHeight="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <androidx.appcompat.widget.AppCompatTextView
        style="@style/XtarWarx.View.Model.Text.Value"
        android:id="@+id/xtarwarx_view_model_specie_description_appcompattextview"
        android:layout_width="@dimen/dp0"
        android:layout_height="@dimen/dp0"
        android:textColor="@color/ColorPrimary"

        app:layout_constraintWidth="match_constraint"
        app:layout_constraintHeight="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/xtarwarx_view_model_specie_name_appcompattextview"/>

    <com.xyclonedesigns.xtarwarx.views.page.ImagesHorizontalPageView
        style="@style/XtarWarx.View.Model.Images"
        android:id="@+id/xtarwarx_view_model_specie_externallinks_imageshorizontalpageview"
        android:layout_width="@dimen/dp0"
        android:layout_height="@dimen/dp0"
        android:layout_marginTop="@dimen/dp32"

        app:isExternalLinks="true"
        app:withPagination="false"
        app:withRefresh="false"
        app:layout_constraintHeight_max="@dimen/dp64"
        app:layout_constraintWidth="match_constraint"
        app:layout_constraintHeight="wrap_content_constrained"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/xtarwarx_view_model_specie_description_appcompattextview"/>

    <com.xyclonedesigns.xtarwarx.views.page.ImagesHorizontalPageView
        style="@style/XtarWarx.View.Model.Images"
        android:id="@+id/xtarwarx_view_model_specie_images_imageshorizontalpageview"
        android:layout_width="@dimen/dp0"
        android:layout_height="@dimen/dp0"
        android:layout_marginVertical="@dimen/dp32"

        app:emptyText="@string/models_specie_emptytext_images"
        app:layout_constraintWidth="match_constraint"
        app:layout_constraintHeight="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/xtarwarx_view_model_specie_externallinks_imageshorizontalpageview"/>

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/xtarwarx_view_model_specie_details_barrier_end"
        android:layout_width="@dimen/dp0"
        android:layout_height="@dimen/dp0"

        app:barrierDirection="end"
        app:constraint_referenced_ids="
            xtarwarx_view_model_specie_language_key_appcompattextview,
            xtarwarx_view_model_specie_classification_key_appcompattextview,
            xtarwarx_view_model_specie_designation_key_appcompattextview,
            xtarwarx_view_model_specie_averageheight_key_appcompattextview,
            xtarwarx_view_model_specie_averagelifespan_key_appcompattextview,
            xtarwarx_view_model_specie_eyecolors_key_appcompattextview,
            xtarwarx_view_model_specie_haircolors_key_appcompattextview,
            xtarwarx_view_model_specie_skincolors_key_appcompattextview,"/>


    <androidx.appcompat.widget.AppCompatTextView
        style="@style/XtarWarx.View.Model.Text.Key"
        android:id="@+id/xtarwarx_view_model_specie_language_key_appcompattextview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/models_specie_title_language"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/xtarwarx_view_model_specie_images_imageshorizontalpageview"/>
    <androidx.appcompat.widget.AppCompatTextView
        style="@style/XtarWarx.View.Model.Text.Value"
        android:id="@+id/xtarwarx_view_model_specie_language_value_appcompattextview"
        android:layout_width="@dimen/dp0"
        android:layout_height="wrap_content"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/xtarwarx_view_model_specie_details_barrier_end"
        app:layout_constraintTop_toBottomOf="@id/xtarwarx_view_model_specie_images_imageshorizontalpageview"/>
    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/xtarwarx_view_model_specie_language_barrier_bottom"
        android:layout_width="@dimen/dp0"
        android:layout_height="@dimen/dp0"

        app:barrierDirection="bottom"
        app:constraint_referenced_ids="
            xtarwarx_view_model_specie_language_key_appcompattextview,
            xtarwarx_view_model_specie_language_value_appcompattextview,"/>
    
    
    <androidx.appcompat.widget.AppCompatTextView
        style="@style/XtarWarx.View.Model.Text.Key"
        android:id="@+id/xtarwarx_view_model_specie_classification_key_appcompattextview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/models_specie_title_classification"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/xtarwarx_view_model_specie_language_barrier_bottom"/>
    <androidx.appcompat.widget.AppCompatTextView
        style="@style/XtarWarx.View.Model.Text.Value"
        android:id="@+id/xtarwarx_view_model_specie_classification_value_appcompattextview"
        android:layout_width="@dimen/dp0"
        android:layout_height="wrap_content"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/xtarwarx_view_model_specie_details_barrier_end"
        app:layout_constraintTop_toBottomOf="@id/xtarwarx_view_model_specie_language_barrier_bottom"/>
    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/xtarwarx_view_model_specie_classification_barrier_bottom"
        android:layout_width="@dimen/dp0"
        android:layout_height="@dimen/dp0"

        app:barrierDirection="bottom"
        app:constraint_referenced_ids="
            xtarwarx_view_model_specie_classification_key_appcompattextview,
            xtarwarx_view_model_specie_classification_value_appcompattextview,"/>
    
    
    <androidx.appcompat.widget.AppCompatTextView
        style="@style/XtarWarx.View.Model.Text.Key"
        android:id="@+id/xtarwarx_view_model_specie_designation_key_appcompattextview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/models_specie_title_designation"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/xtarwarx_view_model_specie_classification_barrier_bottom"/>
    <androidx.appcompat.widget.AppCompatTextView
        style="@style/XtarWarx.View.Model.Text.Value"
        android:id="@+id/xtarwarx_view_model_specie_designation_value_appcompattextview"
        android:layout_width="@dimen/dp0"
        android:layout_height="wrap_content"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/xtarwarx_view_model_specie_details_barrier_end"
        app:layout_constraintTop_toBottomOf="@id/xtarwarx_view_model_specie_classification_barrier_bottom"/>
    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/xtarwarx_view_model_specie_designation_barrier_bottom"
        android:layout_width="@dimen/dp0"
        android:layout_height="@dimen/dp0"

        app:barrierDirection="bottom"
        app:constraint_referenced_ids="
            xtarwarx_view_model_specie_designation_key_appcompattextview,
            xtarwarx_view_model_specie_designation_value_appcompattextview,"/>


  
    <androidx.appcompat.widget.AppCompatTextView
        style="@style/XtarWarx.View.Model.Text.Key"
        android:id="@+id/xtarwarx_view_model_specie_averageheight_key_appcompattextview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/models_specie_title_averageheight"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/xtarwarx_view_model_specie_designation_barrier_bottom"/>
    <androidx.appcompat.widget.AppCompatTextView
        style="@style/XtarWarx.View.Model.Text.Value"
        android:id="@+id/xtarwarx_view_model_specie_averageheight_value_appcompattextview"
        android:layout_width="@dimen/dp0"
        android:layout_height="wrap_content"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/xtarwarx_view_model_specie_details_barrier_end"
        app:layout_constraintTop_toBottomOf="@id/xtarwarx_view_model_specie_designation_barrier_bottom"/>
    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/xtarwarx_view_model_specie_averageheight_barrier_bottom"
        android:layout_width="@dimen/dp0"
        android:layout_height="@dimen/dp0"

        app:barrierDirection="bottom"
        app:constraint_referenced_ids="
            xtarwarx_view_model_specie_averageheight_key_appcompattextview,
            xtarwarx_view_model_specie_averageheight_value_appcompattextview,"/>
    
    
      
    <androidx.appcompat.widget.AppCompatTextView
        style="@style/XtarWarx.View.Model.Text.Key"
        android:id="@+id/xtarwarx_view_model_specie_averagelifespan_key_appcompattextview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/models_specie_title_averagelifespan"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/xtarwarx_view_model_specie_averageheight_barrier_bottom"/>
    <androidx.appcompat.widget.AppCompatTextView
        style="@style/XtarWarx.View.Model.Text.Value"
        android:id="@+id/xtarwarx_view_model_specie_averagelifespan_value_appcompattextview"
        android:layout_width="@dimen/dp0"
        android:layout_height="wrap_content"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/xtarwarx_view_model_specie_details_barrier_end"
        app:layout_constraintTop_toBottomOf="@id/xtarwarx_view_model_specie_averageheight_barrier_bottom"/>
    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/xtarwarx_view_model_specie_averagelifespan_barrier_bottom"
        android:layout_width="@dimen/dp0"
        android:layout_height="@dimen/dp0"

        app:barrierDirection="bottom"
        app:constraint_referenced_ids="
            xtarwarx_view_model_specie_averagelifespan_key_appcompattextview,
            xtarwarx_view_model_specie_averagelifespan_value_appcompattextview,"/>
  
    <androidx.appcompat.widget.AppCompatTextView
        style="@style/XtarWarx.View.Model.Text.Key"
        android:id="@+id/xtarwarx_view_model_specie_eyecolors_key_appcompattextview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/models_specie_title_eyecolors"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/xtarwarx_view_model_specie_averagelifespan_barrier_bottom"/>
    <androidx.appcompat.widget.AppCompatTextView
        style="@style/XtarWarx.View.Model.Text.Value"
        android:id="@+id/xtarwarx_view_model_specie_eyecolors_value_appcompattextview"
        android:layout_width="@dimen/dp0"
        android:layout_height="wrap_content"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/xtarwarx_view_model_specie_details_barrier_end"
        app:layout_constraintTop_toBottomOf="@id/xtarwarx_view_model_specie_averagelifespan_barrier_bottom"/>
    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/xtarwarx_view_model_specie_eyecolors_barrier_bottom"
        android:layout_width="@dimen/dp0"
        android:layout_height="@dimen/dp0"

        app:barrierDirection="bottom"
        app:constraint_referenced_ids="
            xtarwarx_view_model_specie_eyecolors_key_appcompattextview,
            xtarwarx_view_model_specie_eyecolors_value_appcompattextview,"/>
  
  
    <androidx.appcompat.widget.AppCompatTextView
        style="@style/XtarWarx.View.Model.Text.Key"
        android:id="@+id/xtarwarx_view_model_specie_haircolors_key_appcompattextview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/models_specie_title_haircolors"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/xtarwarx_view_model_specie_eyecolors_barrier_bottom"/>
    <androidx.appcompat.widget.AppCompatTextView
        style="@style/XtarWarx.View.Model.Text.Value"
        android:id="@+id/xtarwarx_view_model_specie_haircolors_value_appcompattextview"
        android:layout_width="@dimen/dp0"
        android:layout_height="wrap_content"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/xtarwarx_view_model_specie_details_barrier_end"
        app:layout_constraintTop_toBottomOf="@id/xtarwarx_view_model_specie_eyecolors_barrier_bottom"/>
    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/xtarwarx_view_model_specie_haircolors_barrier_bottom"
        android:layout_width="@dimen/dp0"
        android:layout_height="@dimen/dp0"

        app:barrierDirection="bottom"
        app:constraint_referenced_ids="
            xtarwarx_view_model_specie_haircolors_key_appcompattextview,
            xtarwarx_view_model_specie_haircolors_value_appcompattextview,"/>
  
  
    <androidx.appcompat.widget.AppCompatTextView
        style="@style/XtarWarx.View.Model.Text.Key"
        android:id="@+id/xtarwarx_view_model_specie_skincolors_key_appcompattextview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/models_specie_title_skincolors"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/xtarwarx_view_model_specie_haircolors_barrier_bottom"/>
    <androidx.appcompat.widget.AppCompatTextView
        style="@style/XtarWarx.View.Model.Text.Value"
        android:id="@+id/xtarwarx_view_model_specie_skincolors_value_appcompattextview"
        android:layout_width="@dimen/dp0"
        android:layout_height="wrap_content"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/xtarwarx_view_model_specie_details_barrier_end"
        app:layout_constraintTop_toBottomOf="@id/xtarwarx_view_model_specie_haircolors_barrier_bottom"/>
    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/xtarwarx_view_model_specie_skincolors_barrier_bottom"
        android:layout_width="@dimen/dp0"
        android:layout_height="@dimen/dp0"

        app:barrierDirection="bottom"
        app:constraint_referenced_ids="
            xtarwarx_view_model_specie_skincolors_key_appcompattextview,
            xtarwarx_view_model_specie_skincolors_value_appcompattextview,"/>

    <com.xyclonedesigns.xtarwarx.views.page.ItemsHorizontalPageView
        style="@style/XtarWarx.View.Model.Items"
        android:id="@+id/xtarwarx_view_model_specie_homeworld_itemshorizontalpageview"
        android:layout_width="@dimen/dp0"
        android:layout_height="@dimen/dp0"
        android:layout_marginTop="@dimen/dp32"

        app:withPagination="false"
        app:title="@string/models_specie_title_homeworld"
        app:subtitle="@string/models_specie_description_homeworld"
        app:emptyText="@string/models_specie_emptytext_homeworld"
        app:layout_constraintWidth="match_constraint"
        app:layout_constraintHeight="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/xtarwarx_view_model_specie_skincolors_barrier_bottom"/>

    <com.xyclonedesigns.xtarwarx.views.page.ItemsHorizontalPageView
        style="@style/XtarWarx.View.Model.Items"
        android:id="@+id/xtarwarx_view_model_specie_characters_itemshorizontalpageview"
        android:layout_width="@dimen/dp0"
        android:layout_height="@dimen/dp0"
        android:layout_marginVertical="@dimen/dp32"

        app:withPagination="true"
        app:title="@string/models_specie_title_characters"
        app:subtitle="@string/models_specie_description_characters"
        app:emptyText="@string/models_specie_emptytext_characters"
        app:layout_constraintWidth="match_constraint"
        app:layout_constraintHeight="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/xtarwarx_view_model_specie_homeworld_itemshorizontalpageview"/>

</merge>